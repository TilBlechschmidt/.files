{
    "__COMMENT__ More Information": "https://github.com/mozilla/policy-templates/blob/master/README.md",
    "policies": {
        "AppUpdateURL": "https://localhost",
        "DisableAppUpdate": true,
        "OverrideFirstRunPage": "",
        "OverridePostUpdatePage": "",
        "DisableSystemAddonUpdate": true,
        "DisableProfileImport": false,
        "DisableFirefoxStudies": true,
        "DisableTelemetry": true,
        "DisableFeedbackCommands": true,
        "DisablePocket": true,
        "DisableSetDesktopBackground": false,
        "DisableDeveloperTools": false,
        "DNSOverHTTPS": {
            "Enabled": false,
            "ProviderURL": "",
            "Locked": false
        },
        "FirefoxHome": {
            "Search": false
        },
        "NoDefaultBookmarks": true,
        "OfferToSaveLogins": false,
        "PasswordManagerEnabled": false,
        "WebsiteFilter": {
            "Block": [
                "https://localhost/*"
            ],
            "Exceptions": [
                "https://localhost/*"
            ]
        },
        "Cookies": {
            "Behavior": "reject-tracker",
            "BehaviorPrivateBrowsing": "reject-tracker-and-partition-foreign"
        },
        "Extensions": {
            "Install": [
                "https://addons.mozilla.org/firefox/downloads/file/4049960/default_compact_dark_theme-1.2.xpi",
                "https://addons.mozilla.org/firefox/downloads/latest/ublock-origin/latest.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4317971/darkreader-4.9.88.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4307128/kagi_search_for_firefox-0.7.1.xpi",
                "https://addons.mozilla.org/firefox/downloads/file/4321525/1password_x_password_manager-2.26.1.xpi"
            ],
            "Uninstall": [
                "google@search.mozilla.org",
                "bing@search.mozilla.org",
                "amazondotcom@search.mozilla.org",
                "ebay@search.mozilla.org",
                "twitter@search.mozilla.org"
            ]
        },
        "ExtensionSettings": {
            "uBlock0@raymondhill.net": {
                "default_area": "menupanel"
            }
        },
        "SearchEngines": {
            "PreventInstalls": false,
            "Remove": [
                "DuckDuckGo",
                "Google",
                "Bing",
                "Amazon.com",
                "eBay",
                "Twitter",
                "Wikipedia (en)"
            ],
            "Default": "Kagi"
        },
        "SupportMenu": {
            "Title": "LibreWolf Issue Tracker",
            "URL": "https://codeberg.org/librewolf/issues"
        }
    }
}
